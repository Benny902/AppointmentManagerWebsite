{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended", "google"
        
    ],
    "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "sourceType": "module",
    "allowImportExportEverywhere": true
    },
    "rules": {
        "no-unused-vars": "error"
    }
}
